{
  "name": "dimitar-danailov-portfolio",
  "version": "1.7.0",
  "main": "index.js",
  "repository": "https://github.com/dimitardanailov",
  "author": "Dimitar Danailov<dimityr.danailov@gmail.com>",
  "license": "MIT",
  "description": "Javascript contractor",
  "scripts": {
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json)\"",
    "clean": "yarn clean:public",
    "clean:public": "rm -rf ./public & mkdir -p public",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint . --fix",
    "publish": "yarn clean && yarn build && yarn gulp && firebase deploy --only hosting:dimitar",
    "reports": "npx jscpd ./"
  },
  "jscpd": {
    "threshold": 0.25,
    "reporters": [
      "html",
      "console"
    ],
    "ignore": [
      ".next"
    ],
    "absolute": false,
    "gitignore": true
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-wesbos": "0.0.19",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "^5.0.2",
    "gulp": "^4.0.2",
    "prettier": "^1.19.1",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.9.1",
    "@zeit/next-css": "^1.0.1",
    "next": "^9.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "5.0.0"
  }
}
